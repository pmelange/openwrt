			queue-group@1 {
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				reg = <0xb6000 0x1000>;
				interrupts = <0x19 0x2 0x1a 0x2 0x1b 0x2>;
			};
		};

		usb@22000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl-usb2-dr";
			reg = <0x22000 0x1000>;
			interrupt-parent = <0x2>;
			interrupts = <0x1c 0x2>;
			phy_type = "ulpi";
		};

		sdhci@2e000 {
			status = "okay";
			compatible = "fsl,p1020-esdhc", "fsl,esdhc";
			reg = <0x2e000 0x1000>;
			interrupts = <0x48 0x2>;
			interrupt-parent = <0x2>;
			clock-frequency = <0x9ef21a9>;
		};

		crypto@30000 {
			compatible = "fsl,sec3.3", "fsl,sec3.1", "fsl,sec3.0", "fsl,sec2.4", "fsl,sec2.2", "fsl,sec2.1", "fsl,sec2.0";
			reg = <0x30000 0x10000>;
			interrupts = <0x2d 0x2 0x3a 0x2>;
			interrupt-parent = <0x2>;
			fsl,num-channels = <0x4>;
			fsl,channel-fifo-len = <0x18>;
			fsl,exec-units-mask = <0x97c>;
			fsl,descriptor-types-mask = <0x3a30abf>;
			fsl,multi-host-mode = "primary";
			fsl,channel-remap = <0x3>;
		};

		pic@40000 {
			interrupt-controller;
			#address-cells = <0x0>;
			#interrupt-cells = <0x2>;
			reg = <0x40000 0x40000>;
			compatible = "chrp,open-pic";
			device_type = "open-pic";
			linux,phandle = <0x2>;
		};

		message@41400 {
			compatible = "fsl,p1020-msg", "fsl,mpic-msg";
			reg = <0x41400 0x200>;
			interrupts = <0xb0 0x2 0xb1 0x2 0xb2 0x2 0xb3 0x2>;
			interrupt-parent = <0x2>;
		};

		message@42400 {
			compatible = "fsl,p1020-msg", "fsl,mpic-msg";
			reg = <0x42400 0x200>;
			interrupts = <0xb4 0x2 0xb5 0x2 0xb6 0x2 0xb7 0x2>;
			interrupt-parent = <0x2>;
		};

		timer@41100 {
			compatible = "fsl,mpic-global-timer";
			reg = <0x41100 0x204>;
			interrupts = <0xf7 0x2>;
			interrupt-parent = <0x2>;
		};

		power@e0070 {
			compatible = "fsl,mpc8548-pmc";
			reg = <0xe0070 0x20>;

			soc-clk@24 {
				fsl,pmcdr-mask = <0x80>;
				linux,phandle = <0x5>;
			};

			soc-clk@25 {
				fsl,pmcdr-mask = <0x40>;
			};

			soc-clk@26 {
				fsl,pmcdr-mask = <0x20>;
				linux,phandle = <0x7>;
			};
		};

		msi@41600 {
			compatible = "fsl,p1020-msi", "fsl,mpic-msi";
			reg = <0x41600 0x80>;
			msi-available-ranges = <0x0 0x100>;
			interrupts = <0xe0 0x0 0xe1 0x0 0xe2 0x0 0xe3 0x0 0xe4 0x0 0xe5 0x0 0xe6 0x0 0xe7 0x0>;
			interrupt-parent = <0x2>;
		};

		global-utilities@e0000 {
			compatible = "fsl,p1020-guts", "fsl,p2020-guts";
			reg = <0xe0000 0x1000>;
			fsl,has-rstcr;
		};
	};

	pcie@ffe09000 {
		dma-ranges = <0x2000000 0x0 0xfff00000 0x0 0xffe00000 0x0 0x100000 0x42000000 0x0 0x0 0x0 0x0 0x0 0x8000000>;
		compatible = "fsl,mpc8548-pcie";
