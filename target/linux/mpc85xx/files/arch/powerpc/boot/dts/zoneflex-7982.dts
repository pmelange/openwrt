			queue-group@1 {
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				reg = <0xb4000 0x1000>;
				interrupts = <0x11 0x2 0x12 0x2 0x18 0x2>;
			};
		};

		ethernet@b2000 {
			rx-stash-idx = <0x0>;
			rx-stash-len = <0x60>;
			bd-stash;
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			device_type = "network";
			model = "eTSEC";
			compatible = "fsl,etsec2";
			fsl,num_rx_queues = <0x8>;
			fsl,num_tx_queues = <0x8>;
