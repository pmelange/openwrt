/dts-v1/;

/memreserve/	0x0000000007f00000 0x0000000000100000;
/memreserve/	0x0000000000ff6000 0x0000000000007000;
/ {
	model = "Ruckus ZoneFlex 7982";
	compatible = "ruckus,zoneflex-7982";
	#address-cells = <0x2>;
	#size-cells = <0x2>;

	chosen {
		linux,stdout-path = "/soc@ffe00000/serial@4500";
		bootargs = "console=ttyS0,115200  uboot=1 usb  ";
	};

	aliases {
		serial0 = "/soc@ffe00000/serial@4500";
		serial1 = "/soc@ffe00000/serial@4600";
		ethernet0 = "/soc@ffe00000/ethernet@b0000";
		ethernet1 = "/soc@ffe00000/ethernet@b2000";
		pci0 = "/pcie@ffe09000";
		pci1 = "/pcie@ffe0a000";
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;

		PowerPC,P1024@0 {
			i-cache-sets = <0x80>;
			i-cache-size = <0x8000>;
			i-cache-block-size = <0x20>;
			d-cache-sets = <0x80>;
			d-cache-size = <0x8000>;
			d-cache-block-size = <0x20>;
			clock-frequency = <0x27bc86a4>;
			bus-frequency = <0x13de4352>;
			timebase-frequency = <0x27bc86a>;
			device_type = "cpu";
			reg = <0x0>;
			next-level-cache = <0x1>;
		};
	};

	memory {
		reg = <0x0 0x0 0x0 0x8000000>;
		device_type = "memory";
	};

	soc@ffe00000 {
		#address-cells = <0x1>;
		#size-cells = <0x1>;
		device_type = "soc";
		compatible = "fsl,p1020-immr", "simple-bus";
		ranges = <0x0 0x0 0xffe00000 0x100000>;
		bus-frequency = <0x13de4352>;

		ecm-law@0 {
			compatible = "fsl,ecm-law";
			reg = <0x0 0x1000>;
			fsl,num-laws = <0xc>;
		};

		ecm@1000 {
			compatible = "fsl,p1020-ecm", "fsl,ecm";
			reg = <0x1000 0x1000>;
			interrupts = <0x10 0x2>;
			interrupt-parent = <0x2>;
		};

		memory-controller@2000 {
			compatible = "fsl,p1020-memory-controller";
			reg = <0x2000 0x1000>;
			interrupt-parent = <0x2>;
			interrupts = <0x10 0x2>;
		};

		i2c@3000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl-i2c";
			reg = <0x3000 0x100>;
			interrupts = <0x2b 0x2>;
			interrupt-parent = <0x2>;
			dfsrr;
		};

		i2c@3100 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl-i2c";
			reg = <0x3100 0x100>;
			interrupts = <0x2b 0x2>;
			interrupt-parent = <0x2>;
			dfsrr;
		};

		serial@4500 {
			device_type = "serial";
			compatible = "ns16550";
			reg = <0x4500 0x100>;
			clock-frequency = <0x13de4352>;
			interrupts = <0x2a 0x2>;
			interrupt-parent = <0x2>;
		};

		serial@4600 {
			device_type = "serial";
			compatible = "ns16550";
			reg = <0x4600 0x100>;
			clock-frequency = <0x13de4352>;
			interrupts = <0x2a 0x2>;
			interrupt-parent = <0x2>;
		};

		spi@7000 {
			cell-index = <0x0>;
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl,espi";
			reg = <0x7000 0x1000>;
			interrupts = <0x3b 0x2>;
			interrupt-parent = <0x2>;
			espi,num-ss-bits = <0x4>;
			mode = "cpu";

			fsl_m25p80@0 {
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				compatible = "fsl,espi-flash";
				reg = <0x0>;
				linux,modalias = "fsl_m25p80";
				spi-max-frequency = <0x2625a00>;
			};

			legerity@0 {
				compatible = "fsl,espi-slic";
				reg = <0x1>;
				linux,modalias = "legerity";
				spi-max-frequency = <0x7a1200>;
				ch1-rx-slot = <0x0>;
				ch1-tx-slot = <0x0>;
				ch2-rx-slot = <0x2>;
				ch2-tx-slot = <0x2>;
			};

			legerity@1 {
				compatible = "fsl,espi-slic";
				reg = <0x2>;
				linux,modalias = "legerity";
				spi-max-frequency = <0x7a1200>;
				ch1-rx-slot = <0x4>;
				ch1-tx-slot = <0x4>;
				ch2-rx-slot = <0x6>;
				ch2-tx-slot = <0x6>;
			};
		};

		gpio-controller@f000 {
			#gpio-cells = <0x2>;
			compatible = "fsl,mpc8572-gpio";
			reg = <0xf000 0x100>;
			interrupts = <0x2f 0x2>;
			interrupt-parent = <0x2>;
			gpio-controller;
		};

		tdm@16000 {
			compatible = "fsl,starlite-tdm";
			reg = <0x16000 0x200 0x2c000 0x2000>;
			clock-frequency = <0x0>;
			interrupts = <0x3f 0x8 0x3e 0x8>;
			interrupt-parent = <0x2>;
		};

		l2-cache-controller@20000 {
			cache-level = <0x2>;
			cache-sets = <0x400>;
			cache-block-size = <0x20>;
			cache-unified;
			compatible = [66 73 6c 2c 70 31 30 31 31 2d 6c 32 2d 63 61 63 68 65 2d 63 6f 6e 74 72 6f 6c 6c 65 72 00 63 61 63 68 65 00 07 e7];
			reg = <0x20000 0x1000>;
			cache-line-size = <0x20>;
			cache-size = <0x40000>;
			interrupt-parent = <0x2>;
			interrupts = <0x10 0x2>;
			linux,phandle = <0x1>;
		};

		dma@21300 {
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			compatible = "fsl,eloplus-dma";
			reg = <0x21300 0x4>;
			ranges = <0x0 0x21100 0x200>;
			cell-index = <0x0>;

			dma-channel@0 {
				compatible = "fsl,eloplus-dma-channel";
				reg = <0x0 0x80>;
				cell-index = <0x0>;
				interrupt-parent = <0x2>;
				interrupts = <0x14 0x2>;
			};

			dma-channel@80 {
				compatible = "fsl,eloplus-dma-channel";
				reg = <0x80 0x80>;
				cell-index = <0x1>;
				interrupt-parent = <0x2>;
				interrupts = <0x15 0x2>;
			};

			dma-channel@100 {
				compatible = "fsl,eloplus-dma-channel";
				reg = <0x100 0x80>;
				cell-index = <0x2>;
				interrupt-parent = <0x2>;
				interrupts = <0x16 0x2>;
			};

			dma-channel@180 {
				compatible = "fsl,eloplus-dma-channel";
				reg = <0x180 0x80>;
				cell-index = <0x3>;
				interrupt-parent = <0x2>;
				interrupts = <0x17 0x2>;
			};
		};

		mdio@24000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl,etsec2-mdio";
			reg = <0x24000 0x1000 0xb0030 0x4>;

			ethernet-phy@0 {
				interrupt-parent = <0x2>;
				interrupts = <0x3 0x1>;
				reg = <0x6>;
				linux,phandle = <0x3>;
			};

			ethernet-phy@1 {
				interrupt-parent = <0x2>;
				interrupts = <0x2 0x1>;
				reg = <0x2>;
			};

			ethernet-phy@2 {
				interrupt-parent = <0x2>;
				interrupts = <0x1 0x1>;
				reg = <0x5>;
				linux,phandle = <0x6>;
			};
		};

		mdio@25000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl,etsec2-tbi";
			reg = <0x25000 0x1000 0xb1030 0x4>;

			tbi-phy@11 {
				reg = <0x11>;
				device_type = "tbi-phy";
			};
		};

		mdio@26000 {
			#address-cells = <0x1>;
			#size-cells = <0x0>;
			compatible = "fsl,etsec2-tbi";
			reg = <0x26000 0x1000 0xb1030 0x4>;

			tbi-phy@11 {
				reg = <0x11>;
				device_type = "tbi-phy";
			};
		};

		ptimer@b0e00 {
			timer-frequency = <0x9ef21a9>;
			compatible = "fsl,gianfar-ptp-timer";
			reg = <0xb0e00 0xb0>;
			interrupts = <0x44 0x2 0x45 0x2 0x46 0x2>;
			interrupt-parent = <0x2>;
			tmr-prsc = <0x2>;
			cksel = <0x1>;
			linux,phandle = <0x4>;
		};

		ethernet@b0000 {
			rx-stash-idx = <0x0>;
			rx-stash-len = <0x60>;
			bd-stash;
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			device_type = "network";
			model = "eTSEC";
			compatible = "fsl,etsec2";
			fsl,num_rx_queues = <0x8>;
			fsl,num_tx_queues = <0x8>;
			local-mac-address = [00 19 77 08 10 00];
			interrupt-parent = <0x2>;
			fixed-link = <0x1 0x1 0x3e8 0x0 0x0>;
			phy-handle = <0x3>;
			phy-connection-type = "rgmii-id";
			ptimer-handle = <0x4>;
			fsl,magic-packet;
			clk-handle = <0x5>;

			queue-group@0 {
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				reg = <0xb0000 0x1000>;
				interrupts = <0x1d 0x2 0x1e 0x2 0x22 0x2>;
